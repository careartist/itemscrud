{
    "name": "backpack/itemscrud",
    "type": "library",
    "description": "An admin panel for items with categories and materials, using Backpack\\CRUD on Laravel 5.",
    "keywords": [
        "backpack",
        "ItemsCRUD"
    ],
    "homepage": "https://github.com/backpack/NewsCRUD",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Tabacitu",
            "email": "hello@tabacitu.ro",
            "homepage": "http://tabacitu.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "backpack/crud": "^3.0.6",
        "cviebrock/eloquent-sluggable": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\ItemsCRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\ItemsCRUD\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
